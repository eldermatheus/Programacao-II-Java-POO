package teste;

public class OperadoresAritmeticos {

	public static void main(String[] args) {
		/*int valor1 = 5;
		int valor2 = 10;
		int soma = valor1 + valor2;*/
		int n = 10;
		System.out.println(n + " + 2 = " + (n+2));
		System.out.println(n + " - 2 = " + (n-2));
		System.out.println(n + " * 2 = " + (n*2));
		System.out.println(n + " / 2 = " + (n/2));
		System.out.println(n + " % 2 = " + (n%2));
	
	}
}