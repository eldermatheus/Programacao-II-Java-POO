package teste;

public class LacosRepedicao {
	public static void main(String[] args) {		
		//Uso do For
		for(int i=1; i<=10;i++) 
			for(int j=1; j<=10;j++) {		
				System.out.println("");
				System.out.println(i + " * " + j +" = " + (i*j));
				System.out.println("");
		}
		
		//Uso do
		
		
		
		//Uso do 
		
		
	}
}
